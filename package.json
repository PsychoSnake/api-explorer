{
  "name": "api-explorer",
  "version": "0.0.0",
  "description": "API Explorer (live documentation) for Swagger",
  "scripts": {
    "start": "node server.js",
    "test": "NODE_ENV=test mocha --recursive --compilers js:babel-core/register --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "lint": "standard --verbose | snazzy",
    "lint:watch": "nodemon --watch ./ --ext js --exec npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sky-uk/api-explorer.git"
  },
  "author": "Sky UK Limited",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "bugs": {
    "url": "https://github.com/sky-uk/api-explorer/issues"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "APIExplorer",
      "fetch",
      "describe",
      "it"
    ]
  },
  "dependencies": {
    "classnames": "^2.1.2",
    "history": "^1.12.6",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc3",
    "redux": "^3.0.0",
    "redux-router": "^1.0.0-beta3"
  },
  "devDependencies": {
    "babel-core": "^5.6.18",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.1.4",
    "babel-plugin-react-transform": "^1.1.0",
    "babel-runtime": "^5.8.29",
    "expect": "^1.8.0",
    "express": "^4.13.3",
    "jsdom": "^5.6.1",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "nodemon": "^1.8.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^0.14.0",
    "react-transform-hmr": "^1.0.0",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "snazzy": "^2.0.1",
    "standard": "^5.3.1",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.11",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  }
