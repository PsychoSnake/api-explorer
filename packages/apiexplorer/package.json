    "moment": "^2.18.1",
    "prop-types": "^15.5.8",
    "react-dock": "^0.2.3",
    "react-helmet": "^5.0.3",
    "react-highlight": "^0.9.0",
    "react-jsonschema-form": "^0.46.0",
    "react-redux": "^5.0.4",
    "react-redux-toastr": "^6.2.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
